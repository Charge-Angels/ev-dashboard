{
  "name": "ev-dashboard",
  "version": "2.1.10",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasBrazi06/ev-dashboard-new"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "start:prod": "ng serve --prod",
    "start:ssl": "ng serve --configuration=development --ssl true --ssl-key ./ssl/localhost.key --ssl-cert ./ssl/localhost.cert",
    "build": "npm run build:dev",
    "build:prod": "ng build --prod --stats-json",
    "build:dev": "ng build --configuration=development --stats-json",
    "test": "npm run e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix=true",
    "e2e": "codeceptjs run --steps --reporter mochawesome",
    "clean-install-win": "rmdir node_modules /S /Q && del package-lock.json && npm cache clear --force && npm install",
    "clean-install-lin": "rm -rf ./node_modules && rm -f ./package-lock.json && npm cache clear --force && npm install",
    "npm-check": "npm-check",
    "ng:update": "ng update",
    "angular-update": "ng update @angular/cli @angular/core",
    "material-update": "ng update @angular/material",
    "angular9-update": "ng update @angular/core@9 @angular/cli@9",
    "material9-update": "ng update @angular/material@9",
    "angular-dependencies-update": "ng update --all",
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "postinstall": "ngcc"
  },
