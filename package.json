    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@auth0/angular-jwt": "^3.0.0",
    "@mat-datetimepicker/core": "^3.1.0",
    "@mat-datetimepicker/moment": "^3.1.0",
    "@ng-bootstrap/ng-bootstrap": "^5.2.2",
    "@ngx-translate/core": "^11.0.1",
