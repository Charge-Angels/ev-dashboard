  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --configuration=development",
    "start:prod": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --prod",
    "start:ssl": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng serve --configuration=development --ssl true --ssl-key ./ssl/localhost.key --ssl-cert ./ssl/localhost.cert",
    "build": "npm run build:dev",
    "build:prod": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng build --prod",
    "build:dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" ng build --configuration=development",
    "test": "npm run e2e",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix=true",
    "e2e": "codeceptjs run --steps --reporter mochawesome",
    "clean-install-win": "rmdir node_modules /S /Q && del package-lock.json && npm cache clear --force && npm install",
    "clean-install-lin": "rm -rf ./node_modules && rm -f ./package-lock.json && npm cache clear --force && npm install"
  },
